.gallery {
  margin: 0px 0px 0px 0px;
  background: white;
  padding: 10px;
}

.gallery img {
  margin: 5px;
  padding: 5px;
  /* width: 175px; */
  height: 100px;
  border: solid 1px black;
  -webkit-transition: all 1s ease 0s;
  transition: all 1s ease 0s;
  background: white;
  display:inline
}

.gallery img:hover {
  -webkit-transform: scale(2.2) rotate(0deg);
  transform: scale(2.2) rotate(0deg);
}
